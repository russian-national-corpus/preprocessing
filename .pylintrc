[MASTER]
ignore=Legacy

[MESSAGES CONTROL]
disable=missing-docstring,fixme

[SIMILARITIES]
ignore-imports=yes
min-similarity-lines=10

[BASIC]
class-attribute-naming-style=snake_case
good-names=i,j,k,_
inlinevar-naming-style=snake_case
module-naming-style=PascalCase
no-docstring-rgx=

[VARIABLES]
allow-global-unused-variables=yes
dummy-variables-rgx=
ignored-argument-names=
init-import=yes

[MISCELLANEOUS]
notes=fixme,todo

[SPELLING]
spelling-private-dict-file=

[TYPECHECK]
ignored-classes=

[FORMAT]
expected-line-ending-format=LF
ignore-long-lines=
max-line-length=120
no-space-check=

[CLASSES]
defining-attr-methods=__init__
exclude-protected=
valid-classmethod-first-arg=this
valid-metaclass-classmethod-first-arg=this

[DESIGN]
min-public-methods=1